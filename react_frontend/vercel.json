{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/react_frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/react_frontend/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/react_frontend/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/react_frontend/(.*).woff2",
      "headers": [
        {
          "key": "Content-Type",
          "value": "font/woff2"
        }
      ]
    }
  ]
}
